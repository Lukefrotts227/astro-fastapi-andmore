---
const links = [
    {href: '/', text: 'Home'}, 
    {href: '/about', text: 'About'}, 
    {href: '/entry', text: 'Entry'}, 
]
---


<nav class="bg-gray-700 text-white p-5">
    <div class="container mx-auto flex items-center justify-between">
        <a href="/" class="text-xl font-semibold cursor-pointer">MySite</a>
        <ul class="flex space-x-4">
            {links.map(link => (
                <li>
                    <a href={link.href} class="hover:text-gray-300 cursor-pointer">{link.text}</a>
                </li>
            ))}
        </ul>
    </div>
</nav>